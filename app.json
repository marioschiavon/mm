    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.ico"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}